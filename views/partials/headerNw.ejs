<header class="contenedorHeader">
    <section class="regisUserContainer">
        <a class="linkContainer" href="/login"><img class="userBtn" src="/images/icons/user-icon.svg" alt="userLogin" srcset=""></a>
        <a class="linkContainer" href="/register"><img class="regisBtn" src="/images/icons/register-icon.svg" alt="login" srcset=""></a>
        <form action="/user/logout" method="post">
            <button name="logout" type="submit" class="enviarButton">LOGOUT</button>
        </form>
    </section>
    <section class="headerLogo">
        <a href="/">
            <img src="/images/NwLogo.svg" alt="Logo">
        </a>
    </section>
    <form class="formularioBusqueda" action="/search" method="GET">
        <button class="botonBusqueda">
            <img src="/images/icons/icon-search.svg" alt="searchIcon" srcset="">
        </button>
        <input type="text" name="search" value="" class="searchInput">
    </form>
</header>